    "@ionic-native/firebase-authentication": "^5.26.0",
    "@ionic-native/google-plus": "^5.26.0",
    "@ionic-native/keyboard": "^5.26.0",
    "@ionic-native/status-bar": "^5.26.0",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.2",
    "capacitor-stripe": "^0.4.1",
    "cordova-plugin-facebook4": "^6.4.0",
    "cordova-plugin-firebase-authentication": "^3.3.0",
    "cordova-plugin-googleplus": "^8.5.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-support-android-plugin": "^1.0.2",
    "cordova-support-google-services": "^1.4.0",
    "env-cmd": "^10.1.0",
    "firebase": "^7.14.4",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-http-backend": "^1.0.15",
    "ionic-plugin-deeplinks": "^1.0.20",
    "ionicons": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "patch-package": "^6.2.2",
    "react": "^16.13.0",
    "react-country-flag": "^2.1.0",
    "react-dom": "^16.13.0",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.5.0",
    "react-iframe": "^1.8.0",
    "react-redux": "^7.2.0",
    "react-redux-firebase": "^3.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux-firestore": "^0.13.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build:dev": "env-cmd -f .env react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "lint": "eslint 'src/*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "patch-package",
    "run-ios": "ionic capacitor run ios",
    "run-android": "ionic capacitor run android",
    "run-npx-sync": "npx cap copy && npx cap sync",
    "update:plist:dev" : "/usr/libexec/PlistBuddy -c 'delete CFBundleURLTypes'  './ios/App/App/Info.plist' && /usr/libexec/PlistBuddy -c 'Merge ./resources/Info-dev.plist' './ios/App/App/Info.plist'",
    "update:plist:prod" : "/usr/libexec/PlistBuddy -c 'delete CFBundleURLTypes'  './ios/App/App/Info.plist' && /usr/libexec/PlistBuddy -c 'Merge ./resources/Info-prod.plist' './ios/App/App/Info.plist'",
    "copy:resource:ios:dev" : "cp resources/GoogleService-Info-dev.plist ios/App/App/GoogleService-Info.plist && npm run update:plist:dev",
    "copy:resource:android:dev" : "cp resources/google-services-dev.json android/app/google-services.json",
    "copy:resource:ios:prod" : "cp resources/GoogleService-Info-prod.plist ios/App/App/GoogleService-Info.plist && npm run update:plist:prod",
    "copy:resource:android:prod" : "cp resources/google-services-prod.json android/app/google-services.json",
    "run-ios:dev": "npm run copy:resource:ios:dev && env-cmd -f .env npm run run-ios",
    "run-android:dev": "npm run copy:resource:android:dev && env-cmd -f .env npm run run-android",
    "run-ios:prod": "npm run copy:resource:ios:prod && env-cmd -f .env.production npm run run-ios",
    "run-android:prod": "npm run copy:resource:android:prod && env-cmd -f .env.production npm run run-android",
    "deploy:dev": "env-cmd -f .env react-scripts build --platform=web && cp -Rv build/* fbhost/public && cd fbhost && firebase deploy -P development --only hosting:development ",
    "deploy:prod": "env-cmd -f .env.production react-scripts build --platform=web && cp -Rv build/* fbhost/public && cd fbhost && firebase deploy -P production --only hosting:production ",
    "build:app:ios:dev": "npm run run-npx-sync && npm run run-ios:dev",
    "build:app:android:dev": "npm run run-npx-sync && npm run run-android:dev",
    "build:app:ios:prod": "npm run run-npx-sync && npm run run-ios:prod",
    "build:app:android:prod": "npm run run-npx-sync && npm run run-android:prod",
    "deploy": "npm run deploy:dev && npm run deploy:prod",
    "build-privacy-page": "cd privacy-page && firebase deploy --only hosting:slashiee-privacy ",
    "build-chat-page": "cd slashiee-chat && firebase deploy --only hosting:slashiee-chat "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git add ."
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "An Ionic project",
  "devDependencies": {
    "@capacitor/cli": "2.1.0",
    "@types/lodash": "^4.14.154",
    "@types/react-redux": "^7.1.9",
    "redux-devtools-extension": "^2.13.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  }
}
