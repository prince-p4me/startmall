{
  "hosting": {
    "site": "startmall",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }, {
      "source": "/foo{,/**}",
      "destination": "/index.html"
    }, {
      "source": "!/@(js|css)/**",
      "destination": "/index.html"
    } ],
    "headers": [
      {
        "source": "/.well-known/apple-app-site-association",
        "headers": [{"key": "Content-Type", "value": "application/json"}]
      }
    ],
    "appAssociation": "NONE"
  }
}
